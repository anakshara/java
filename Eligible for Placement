import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) 
    {
        Scanner eg = new Scanner(System.in);

        String name = eg.nextLine();
        int arrears = eg.nextInt();
        int cgpa = eg.nextInt();

        System.out.println("Name of the Student:" + name);

        if ((arrears == 1 && cgpa > 70) || ((arrears == 2) && cgpa > 75)) {
            System.out.println(name + " is Eligible for Placement");
        } else {
            System.out.println(name + " is Not Eligible for Placement");
        }


    }
}
