import java.io.*;
import java.util.*;

public class Solution {
    public static void main(String[] args) {
        Scanner CN = new Scanner(System.in);
        int num = CN.nextInt();
        if(num>0){
            System.out.println("positive");
        } 
         else if(num<0){
            System.out.println("negative");
            
        } 
          else{
            System.out.println("zero");
        }
        
    }
}
